{
  "name": "fast-cli",
  "version": "4.0.0",
  "description": "Test your download and upload speed using fast.com",
  "license": "MIT",
  "repository": "sindresorhus/fast-cli",
  "funding": "https://github.com/sponsors/sindresorhus",
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "https://sindresorhus.com"
  },
  "type": "module",
  "main": "cli.js",
  "bin": {
    "fast": "cli.js"
  },
  "engines": {
    "node": ">=12.20"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "xo && ava"
